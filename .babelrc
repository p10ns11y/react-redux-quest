{
  "plugins": [
    ["transform-class-properties", { "loose": true }],
    "transform-object-rest-spread"
  ],
  "presets": [
    ["env", { "loose": true, "modules": false }],
    "react"
  ],

  "env": {
    "test": {
      "presets": [
        ["env", {"loose": true}]
      ],
      "plugins": [
        ["add-module-exports"]
      ]
    },
    "cjs": {
      "presets": [
        ["env", {"loose": true}]
      ],
      "plugins": [
        "add-module-exports",
        ["transform-class-properties", { "loose": true }],
      ]
    },
    "es": {
      "presets": [
        ["env", {"loose": true, "modules": false}]
      ],
      "plugins": [
        ["transform-class-properties", { "loose": true }],
        "transform-object-rest-spread"
      ]
    }
  }
}
